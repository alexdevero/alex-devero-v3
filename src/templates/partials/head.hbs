<meta name="viewport" content="{{{ meta.basic.viewport }}}">
<meta name="handheldFriendly" content="{{{ meta.basic.handheldFriendly }}}">
<meta name="description" content="{{#if lang_cs}}{{{ meta.basic.description-cs }}}{{else}}{{{ meta.basic.description }}}{{/if}}"><!-- Maximum length 155 characters -->
<meta name="subject" content="{{#if lang_cs}}{{{ meta.basic.subject-cs }}}{{else}}{{{ meta.basic.subject }}}{{/if}}">
<meta name="language" content="{{#if lang_cs}}{{{ meta.basic.language-cs }}}{{else}}{{{ meta.basic.language }}}{{/if}}">
<meta name="robots" content="{{{ meta.basic.robots }}}">
<meta name="googlebot" content="{{{ meta.basic.googlebot }}}">
<meta name="classification" content="{{{ meta.basic.classification }}}">
<meta name="url" content="{{{ meta.basic.url }}}">
<meta name="identifier-URL" content="{{{ meta.basic.identifier-URL }}}">
<meta name="coverage" content="{{{ meta.basic.coverage }}}">
<meta name="distribution" content="{{{ meta.basic.distribution }}}">
<meta name="rating" content="{{{ meta.basic.rating }}}">
<link rel="author" href="{{{ meta.basic.author }}}">

<!-- Favicons -->
<link rel="apple-touch-icon" sizes="180x180" href="{{{ meta.favicon.apple-touch-icon }}}">
<link rel="icon" type="image/png" href="{{{ meta.favicon.icon-32 }}}" sizes="32x32">
<link rel="icon" type="image/png" href="{{{ meta.favicon.icon-16 }}}" sizes="16x16">
<link rel="manifest" href="{{{ meta.favicon.manifest }}}">
<link rel="mask-icon" href="{{{ meta.favicon.mask-icon }}}" color="#d30320">
<link rel="shortcut icon" href="{{{ meta.favicon.shortcut-icon }}}">
<meta name="msapplication-config" content="{{{ meta.favicon.msapplication-config }}}">
<meta name="theme-color" content="{{{ meta.favicon.theme-color }}}">

<!-- Facebook OpenGraph tags -->
<meta property="og:title" content="{{#if lang_cs}}{{{ meta.basic.subject-cs }}}{{else}}{{{ meta.basic.subject }}}{{/if}}">
<meta property="og:type" content="{{{ meta.cards-facebook.type }}}">
<meta property="og:description" content="{{#if lang_cs}}{{{ meta.basic.description-cs }}}{{else}}{{{ meta.basic.description }}}{{/if}}">
<meta property="og:image" content="{{ meta.cards-facebook.image }}"><!-- my photo -->
<meta property="og:url" content="{{{ meta.cards-facebook.url }}}">
<meta property="og:locale" content="{{#if lang_cs}}{{{ meta.cards-facebook.locale-cs }}}{{else}}{{{ meta.cards-facebook.locale }}}{{/if}}">
<meta property="article:author" content="{{{ meta.cards-facebook.article-author }}}">

<!-- Twitter cards tags -->
<meta name="twitter:card" content="{{{ meta.cards-twitter.card }}}">
<meta name="twitter:site" content="{{{ meta.cards-twitter.site }}}">
<meta name="twitter:creator" content="{{{ meta.cards-twitter.creator }}}">
<meta name="twitter:url" content="{{{ meta.cards-twitter.url }}}">
<meta name="twitter:title" content="{{#if lang_cs}}{{{ meta.basic.subject-cs }}}{{else}}{{{ meta.basic.subject }}}{{/if}}">
<meta name="twitter:description" content="{{#if lang_cs}}{{{ meta.basic.description-cs }}}{{else}}{{{ meta.basic.description }}}{{/if}}">
<meta name="twitter:image" content="{{{ meta.cards-twitter.image }}}">

<!--  Google+ / Schema.org -->
<meta itemprop="name" content="{{#if lang_cs}}{{{ meta.basic.subject-cs }}}{{else}}{{{ meta.basic.subject }}}{{/if}}">
<meta itemprop="description" content="{{#if lang_cs}}{{{ meta.basic.description-cs }}}{{else}}{{{ meta.basic.description }}}{{/if}}">
<meta itemprop="image" content="{{{ meta.cards-google.image }}}">

<!-- Disable automatic detection and formatting of possible phone numbers -->
<meta name="format-detection" content="telephone=no">

<!-- Font Raleway -->
<link rel="preload" href="{{{ stylesheets.stylesheet-font-raleway }}}" as="font">

<!-- Font Awesome -->
<link rel="stylesheet" href="{{{ stylesheets.stylesheet-font-awesome }}}">

<!-- Stylesheets -->
<link class="js-stylesheet-main" rel="stylesheet" href="{{{ stylesheets.stylesheet-main }}}">

<noscript>
  <link rel="stylesheet" href="{{{ stylesheets.stylesheet-font-awesome }}}">

  <link rel="stylesheet" href="{{{ stylesheets.stylesheet-font-raleway }}}">

  <style>
    html {
      display: block;
      opacity: 1;
    }
  </style>
</noscript>

