<!-- Scripts -->
{{> google-analytics }}

{{#if has-emergence }}
<!-- Emergence.js: https://github.com/xtianmiller/emergence.js -->
<script src="./js/plugins/emergence.min.js"></script>
{{/if}}
{{#if has-jquery }}
<!-- jQuery library hosted on CDN -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script async>
  if (window.jQuery === undefined) {
    var jqueryEl = document.createElement('script');
    jqueryEl.async = true;
    jqueryEl.src = './js/vendor/jquery.min.js';
    var bodyEl = document.querySelector('body');
    bodyEl.appendChild(jqueryEl);
  }
</script>
{{/if}}
{{#if has-lazyload }}
<!-- Vanilla-lazyload: https://verlok.github.io/lazyload -->
<script>
  window.lazyLoadOptions = {
    class_loading: 'img-loading',
    data_src: 'src',
    data_srcset: 'srcset'
  };
</script>
<!-- Download the script and execute it after lazyLoadOptions is defined -->
{{!-- <script async src="https://cdnjs.cloudflare.com/ajax/libs/vanilla-lazyload/8.2.0/lazyload.min.js"></script> --}}
