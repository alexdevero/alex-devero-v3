<!doctype html>
<html lang="en" class="no-js" itemscope itemtype="http://schema.org/WebPage">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  {{> page-title active_ai=true }}

  {{> head active_ai=true lang_cs=false }}
</head>
<body class="page--home">
  {{> browser-notification }}

  {{> manifest }}

  <main>
    {{> header active_ai=true lang_cs=false }}

    <section id="sec__main" class="sec__main hero hero--main hero--big">
      {{> shoal-fish }}
    </section>

    {{> footer }}
  </main>

  {{> scripts }}
</body>
</html>
